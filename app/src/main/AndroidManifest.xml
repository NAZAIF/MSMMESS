<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nazaif.msmmess">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/msm3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".Splash"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>

        <activity android:name=".NameList"
            android:theme="@style/AppTheme"
            android:label="Choose your Name">
        </activity>

        <activity android:name=".MemberMessDetails"
            android:theme="@style/AppTheme"
            android:label="Your Status">
        </activity>

        <activity android:name=".AddName"
            android:label="@string/AddnameText"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>